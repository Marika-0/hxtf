{
	"whitespace": {
		"whilePolicy": "after",
		"typeParamOpenPolicy": "none",
		"closingBracketPolicy": "none",
		"ternaryPolicy": "around",
		"functionTypeHaxe3Policy": "none",
		"typeHintColonPolicy": "none",
		"commaPolicy": "onlyAfter",
		"addLineCommentSpace": true,
		"typeCheckColonPolicy": "around",
		"arrowFunctionsPolicy": "around",
		"openingBracketPolicy": "noneAfter",
		"tryPolicy": "after",
		"typeParamClosePolicy": "none",
		"objectFieldColonPolicy": "after",
		"typeExtensionPolicy": "after",
		"semicolonPolicy": "onlyAfter",
		"functionTypeHaxe4Policy": "around",
		"catchPolicy": "after",
		"binopPolicy": "around",
		"compressSuccessiveParenthesis": true,
		"forPolicy": "after",
		"colonPolicy": "none",
		"doPolicy": "after",
		"switchPolicy": "after",
		"caseColonPolicy": "onlyAfter",
		"parenConfig": {
			"funcParamParens": {
				"removeInnerWhenEmpty": true,
				"closingPolicy": "onlyAfter",
				"openingPolicy": "none"
			},
			"forLoopParens": {
				"removeInnerWhenEmpty": true,
				"closingPolicy": "onlyAfter",
				"openingPolicy": "noneAfter"
			},
			"anonFuncParamParens": {
				"removeInnerWhenEmpty": true,
				"closingPolicy": "onlyAfter",
				"openingPolicy": "none"
			},
			"metadataParens": {
				"removeInnerWhenEmpty": true,
				"closingPolicy": "onlyAfter",
				"openingPolicy": "none"
			},
			"conditionParens": {
				"removeInnerWhenEmpty": true,
				"closingPolicy": "onlyAfter",
				"openingPolicy": "noneAfter"
			},
			"callParens": {
				"removeInnerWhenEmpty": true,
				"closingPolicy": "onlyAfter",
				"openingPolicy": "none"
			},
			"expressionParens": {
				"removeInnerWhenEmpty": true,
				"closingPolicy": "onlyAfter",
				"openingPolicy": "noneAfter"
			}
		},
		"ifPolicy": "after",
		"intervalPolicy": "none",
		"formatStringInterpolation": true,
		"dotPolicy": "none",
		"bracesConfig": {
			"unknownBraces": {
				"removeInnerWhenEmpty": true,
				"closingPolicy": "onlyAfter",
				"openingPolicy": "before"
			},
			"anonTypeBraces": {
				"removeInnerWhenEmpty": true,
				"closingPolicy": "onlyAfter",
				"openingPolicy": "before"
			},
			"blockBraces": {
				"removeInnerWhenEmpty": true,
				"closingPolicy": "onlyAfter",
				"openingPolicy": "before"
			},
			"objectLiteralBraces": {
				"removeInnerWhenEmpty": true,
				"closingPolicy": "onlyAfter",
				"openingPolicy": "before"
			},
			"typedefBraces": {
				"removeInnerWhenEmpty": true,
				"closingPolicy": "onlyAfter",
				"openingPolicy": "before"
			}
		}
	},
	"indentation": {
		"character": "    ",
		"indentComplexValueExpressions": false,
		"indentCaseLabels": true,
		"trailingWhitespace": false,
		"tabWidth": 4,
		"indentObjectLiteral": true,
		"conditionalPolicy": "aligned"
	},
	"sameLine": {
		"forBody": "next",
		"tryBody": "next",
		"anonFunctionBody": "same",
		"whileBody": "next",
		"returnBody": "same",
		"elseIf": "same",
		"expressionIf": "same",
		"ifBody": "next",
		"ifElse": "same",
		"elseBody": "next",
		"expressionCase": "keep",
		"comprehensionFor": "same",
		"expressionIfWithBlocks": false,
		"untypedBody": "same",
		"doWhile": "same",
		"expressionTry": "same",
		"tryCatch": "same",
		"functionBody": "next",
		"caseBody": "next",
		"doWhileBody": "next",
		"returnBodySingleLine": "same",
		"catchBody": "next"
	},
	"disableFormatting": false,
	"emptyLines": {
		"betweenSingleLineTypes": 0,
		"beforeBlocks": "remove",
		"betweenTypes": 1,
		"afterLeftCurly": "remove",
		"afterReturn": "remove",
		"maxAnywhereInFile": 1,
		"enumAbstractEmptyLines": {
			"afterVars": 1,
			"existingBetweenFields": "keep",
			"betweenFunctions": 1,
			"beginType": 0,
			"endType": 0,
			"betweenVars": 0
		},
		"afterFileHeaderComment": 1,
		"lineCommentsBetweenFunctions": "keep",
		"beforePackage": 0,
		"typedefEmptyLines": {
			"betweenFields": 0,
			"existingBetweenFields": "keep",
			"beginType": 0,
			"endType": 0
		},
		"interfaceEmptyLines": {
			"afterVars": 0,
			"existingBetweenFields": "keep",
			"betweenFunctions": 0,
			"beginType": 0,
			"endType": 0,
			"betweenVars": 0
		},
		"externClassEmptyLines": {
			"afterVars": 0,
			"existingBetweenFields": "keep",
			"betweenFunctions": 0,
			"beginType": 0,
			"endType": 0,
			"betweenVars": 0
		},
		"abstractEmptyLines": {
			"betweenStaticFunctions": 1,
			"afterStaticFunctions": 1,
			"afterVars": 1,
			"existingBetweenFields": "keep",
			"betweenFunctions": 1,
			"afterPrivateFunctions": 1,
			"beginType": 0,
			"afterStaticVars": 1,
			"afterPrivateVars": 1,
			"endType": 0,
			"betweenStaticVars": 0,
			"betweenVars": 0
		},
		"afterBlocks": "remove",
		"betweenMultilineComments": 0,
		"importAndUsing": {
			"beforeType": 1,
			"betweenImports": 0,
			"beforeUsing": 1,
			"betweenImportsLevel": "all"
		},
		"classEmptyLines": {
			"betweenStaticFunctions": 1,
			"afterStaticFunctions": 1,
			"afterVars": 1,
			"existingBetweenFields": "keep",
			"betweenFunctions": 1,
			"afterPrivateFunctions": 1,
			"beginType": 0,
			"afterStaticVars": 1,
			"afterPrivateVars": 1,
			"endType": 0,
			"betweenStaticVars": 0,
			"betweenVars": 0
		},
		"lineCommentsBetweenTypes": "keep",
		"afterFieldsWithDocComments": "one",
		"beforeDocCommentEmptyLines": "one",
		"finalNewline": true,
		"enumEmptyLines": {
			"betweenFields": 0,
			"existingBetweenFields": "keep",
			"beginType": 0,
			"endType": 0
		},
		"afterPackage": 1,
		"macroClassEmptyLines": {
			"betweenStaticFunctions": 1,
			"afterStaticFunctions": 1,
			"afterVars": 1,
			"existingBetweenFields": "keep",
			"betweenFunctions": 1,
			"afterPrivateFunctions": 1,
			"beginType": 0,
			"afterStaticVars": 1,
			"afterPrivateVars": 1,
			"endType": 0,
			"betweenStaticVars": 0,
			"betweenVars": 0
		},
		"conditionalsEmptyLines": {
			"afterElse": 0,
			"afterError": 0,
			"beforeError": 0,
			"beforeElse": 0,
			"afterIf": 0,
			"beforeEnd": 0
		},
		"beforeRightCurly": "remove"
	},
	"lineEnds": {
		"objectLiteralCurly": null,
		"emptyCurly": "noBreak",
		"rightCurly": "both",
		"metadataOther": "none",
		"leftCurly": "after",
		"anonTypeCurly": null,
		"metadataType": "none",
		"typedefCurly": null,
		"sharp": "after",
		"metadataVar": "none",
		"blockCurly": null,
		"caseColon": "after",
		"metadataFunction": "none"
	},
	"excludes": [
		"\\.haxelib",
		"\\.git",
		"node_modules"
	],
	"wrapping": {
		"metadataCallParameter": {
			"rules": [
				{
					"type": "fillLine",
					"conditions": [
						{
							"cond": "totalItemLength >= n",
							"value": 140
						}
					]
				},
				{
					"type": "fillLine",
					"conditions": [
						{
							"cond": "lineLength >= n",
							"value": 160
						}
					]
				}
			],
			"defaultWrap": "noWrap"
		},
		"callParameter": {
			"rules": [
				{
					"type": "fillLine",
					"conditions": [
						{
							"cond": "itemCount >= n",
